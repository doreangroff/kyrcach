<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:kyrcach="clr-namespace:kyrcach"
             xmlns:entitites="clr-namespace:kyrcach.Entities"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="kyrcach.Windows.DriversWin"
             Background="White">
    <UserControl.Resources>
        <kyrcach:ImageConverter x:Key="ImageConverter" />
    </UserControl.Resources>
    <ListBox Name="DriverLBox">
        <ListBox.ItemTemplate>
            <DataTemplate DataType="kyrcach:Driver"></DataTemplate>
        </ListBox.ItemTemplate>
    </ListBox>
</UserControl>
